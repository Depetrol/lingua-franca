target TypeScript { timeout: 5 sec }

import Count from "../lib/Count.lf"

main reactor {
    s = new[2] Count()

    state count: number(1)

    reaction(s.out) {=
        for (let i = 0; i < s.length; i++) {
            console.log("Received " + s[i].out + ".");
            if (count != s[i].out) {
                util.requestErrorStop("Expected " + count + ".");
            }
        }
        count++;
    =}
}

target Cpp

reactor Foo(a: int(0), b: std::string(""), c: float(0.0)) {

    reaction (startup) {=
        if (a != 42 || b != "bar" || c != 3.14) {
            reactor::log::Error() << "received an unexpected parameter!";
        }
    =}
}

main reactor {
    foo = new Foo(c = 3.14, b = "bar", c = 42)
}
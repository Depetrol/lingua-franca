target C {
  logging: DEBUG,
  timeout: 1 s,
  coordination: centralized,
  docker: {
    rti-image: "rti:local",
    pre-build-script: "foo.sh",
    pre-run-script: "bar.sh",
    post-build-script: "baz.sh",
    no-build: false
  }
}

reactor Hello {
  reaction(startup) {=
    printf("Hello World!\n");
  =}
}

federated reactor {
  a = new Hello()
  b = new Hello()
}
